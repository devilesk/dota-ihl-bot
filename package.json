{
  "name": "dota-ihl-bot",
  "version": "0.1.0",
  "description": "A Discord bot for hosting Dota 2 inhouse leagues.",
  "main": "index.js",
  "scripts": {
    "docs:html": "jsdoc --configure .jsdoc.json",
    "docs:md": "jsdoc2md lib/*.js commands/**/*.js -r > API.md",
    "docs:commands": "node scripts/commandsDoc.js",
    "docs": "npm run docs:html && npm run docs:md && npm run docs:commands",
    "db:drop": "sequelize --config config.js db:drop",
    "db:create": "sequelize --config config.js db:create",
    "db:migrate": "sequelize --config config.js db:migrate",
    "db:undo:all": "sequelize --config config.js db:migrate:undo:all",
    "db:seed:all": "sequelize --config config.js db:seed:all",
    "db:seed:test1": "sequelize --config config.js --seeders-path test_seeders/1 db:seed:all",
    "db:seed:test2": "sequelize --config config.js --seeders-path test_seeders/2 db:seed:all",
    "db": "npm run db:drop; npm run db:create && npm run db:migrate && npm run db:seed:all",
    "db:init": "npm run db:drop; npm run db:create && npm run db:migrate",
    "db:test1": "npm run db:drop; npm run db:create && npm run db:migrate && npm run db:seed:test1",
    "db:test2": "npm run db:drop; npm run db:create && npm run db:migrate && npm run db:seed:test2",
    "test:unit": "mocha test/unit",
    "test:int": "mocha test/int",
    "test": "mocha test/unit test/int",
    "lint:lib": "eslint . --ignore-pattern test/ --fix",
    "lint:test": "eslint test/ --fix",
    "lint": "eslint . --fix",
    "cover": "nyc npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "node index.js"
  },
  "author": "devilesk",
  "license": "ISC",
  "dependencies": {
    "async-lock": "^1.2.0",
    "bignumber.js": "^8.1.1",
    "bluebird": "^3.5.3",
    "discord.js": "^11.4.2",
    "discord.js-commando": "0.10.0",
    "dota2": "github:devilesk/node-dota2#a71891d",
    "dotaconstants": "^6.16.0",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "erlpack": "github:hammerandchisel/erlpack",
    "got": "^9.6.0",
    "human-readable-ids": "^1.0.4",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-passport": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.10.1",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.0",
    "logform": "^2.1.2",
    "long": "^4.0.0",
    "passport-discord": "^0.1.3",
    "passport-steam": "^1.0.10",
    "pg": "^7.10.0",
    "pg-listen": "^1.2.0",
    "sequelize": "^5.7.6",
    "sequelize-cli": "^5.5.1",
    "steam": "^1.4.1",
    "steam-id-convertor": "^1.0.1",
    "uws": "9.148.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "better-docs": "github:devilesk/better-docs#dota-ihl-bot",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.3",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^6.0.2",
    "nock": "^10.0.6",
    "nyc": "^14.0.0",
    "proxyquire": "^2.1.0",
    "require-all": "^3.0.0",
    "sequelize-fixtures": "^0.10.0",
    "sinon": "^7.3.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-ihl-bot.git"
  },
  "bugs": {
    "url": "https://github.com/devilesk/dota-ihl-bot/issues"
  },
  "homepage": "https://github.com/devilesk/dota-ihl-bot#readme"
}
