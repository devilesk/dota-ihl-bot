{
  "name": "dota-ihl-bot",
  "version": "0.0.0",
  "description": "A discord bot for hosting Dota 2 inhouse leagues",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install steam",
    "docs:html": "jsdoc --configure .jsdoc.json",
    "docs:md": "jsdoc2md lib/*.js commands/**/*.js -r > api.md",
    "docs": "npm run docs:html && npm run docs:md",
    "db:drop": "sequelize --config config.js db:drop;",
    "db:create": "sequelize --config config.js db:create;",
    "db:migrate": "sequelize --config config.js db:migrate;",
    "db:seed:all": "sequelize --config config.js db:seed:all;",
    "db:seed:test1": "sequelize --config config.js --seeders-path test_seeders/1 db:seed:all;",
    "db:seed:test2": "sequelize --config config.js --seeders-path test_seeders/2 db:seed:all;",
    "db": "npm run db:create && npm run db:migrate && npm run db:seed:all",
    "db:test1": "npm run db:create && npm run db:migrate && npm run db:seed:test1",
    "db:test2": "npm run db:create && npm run db:migrate && npm run db:seed:test2",
    "test": "mocha test/unit test/int",
    "test:unit": "mocha test/unit",
    "test:int": "mocha test/int",
    "start": "node index.js"
  },
  "author": "devilesk",
  "license": "ISC",
  "dependencies": {
    "async-lock": "^1.2.0",
    "bignumber.js": "^8.1.1",
    "bluebird": "^3.5.3",
    "discord.js": "^11.4.2",
    "discord.js-commando": "0.10.0",
    "dota2": "^6.2.0",
    "dotaconstants": "^6.16.0",
    "dotenv": "^7.0.0",
    "erlpack": "github:hammerandchisel/erlpack",
    "express": "^4.16.4",
    "express-session": "^1.13.0",
    "human-readable-ids": "^1.0.4",
    "logform": "^1.10.0",
    "passport": "^0.4.0",
    "passport-discord": "^0.1.3",
    "pg": "^7.9.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sequelize": "^4.43.0",
    "sequelize-cli": "^5.2.6",
    "steam": "^1.4.1",
    "steam-id-convertor": "^1.0.1",
    "steamid": "^1.1.0",
    "steamidconvert": "^0.2.4",
    "uws": "^9.14.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jsdoc": "^3.5.5",
    "jsdoc-to-markdown": "^4.0.1",
    "long": "^4.0.0",
    "mocha": "^6.0.2",
    "nyc": "^13.3.0",
    "proxyquire": "^2.1.0",
    "sequelize-mocking": "^1.4.1",
    "sinon": "^7.3.1",
    "tui-jsdoc-template": "^1.2.2"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devilesk/dota-ihl-bot.git"
  },
  "bugs": {
    "url": "https://github.com/devilesk/dota-ihl-bot/issues"
  },
  "homepage": "https://github.com/devilesk/dota-ihl-bot#readme"
}
